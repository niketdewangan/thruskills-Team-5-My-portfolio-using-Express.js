 <main>
    <div class="container mb-5">
      
      <div class="row ">
        {{#each project}}
        <div class="col-lg-4 my-4">
          <div class="card    ">
            <img class="card-img-top" src="{{img_1}}" alt="Card image">
            <div class="overlay-img card-bg">
              <div class="card-logo">
                <h3 class="text-white text-center">{{title}}</h3>

              </div>
              <a href="/project/{{_id}}">
                <div class="overlay">
                  <h5 class="text-light project-title ">{{title}}</h5>
                  <p class="text-light project-desc">{{desc}}</p>
                </div>
              </a>
            </div>
          </div>
        </div>
        {{/each}}
        
      </div>

    </div>
  </main>